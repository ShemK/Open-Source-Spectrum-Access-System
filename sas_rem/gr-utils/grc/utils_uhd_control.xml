  <make>utils.uhd_control()</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
 <source>
    <name>out</name>
    <type>float</type>
  </source>
  <source>
    <name>control</name>
    <type>message</type>
  </source>
</block>

